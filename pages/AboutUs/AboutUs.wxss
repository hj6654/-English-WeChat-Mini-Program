.content{
  background: #feeeed;height: 100vh;display: flex;flex-direction: column;
}
.topContent{
  display: flex;
  flex-direction: column;
  align-items: center;
  padding-top: 60rpx;
}
.topContent image{
  overflow: hidden;
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.line {
  width: 95%;
  height: 3rpx;
  background-color: #ccc;
  margin: 60rpx auto;
}

.middleContent{
  margin-bottom: 400rpx;width: 90%;margin:0 20rpx;text-align: justify;color: #696969;display: flex;justify-self: center;flex-direction: column;
  align-self: center;
}
.bottomContent{
text-align: center;margin-top: 60rpx;font-size: 40rpx;
}